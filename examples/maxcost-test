
  for cost in {5..10}; do
    timeout 10s time java -jar ../xhail-0.5.2/xhail.jar -b -c /home/travis/miniconda/envs/test/bin/clasp -g /home/travis/miniconda/envs/test/bin/gringo $ex $cost 2>&1 | grep 'ERROR\|meaningful\|Caused by\|No match found' &> /dev/null
    if [ $? != 0 ]; then
      echo "maxcost found for $ex"
      break
    fi
    if [ $cost == 10 ]; then
      echo "unsatisfiable example: $ex"
    fi
  done
done
